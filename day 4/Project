pragma solidity ^0.4.17 <0.7.0;

contract studentDetails {
    
    string Name;
    uint RollNo;
    uint  Batch;
    uint  Marks;
    string  Status;
    
    
    function Student(string newname,uint newRollnumber, uint newBatch,uint newmarks,string newstatus) public{
        Name=newname;
        RollNo=newRollnumber;
        Batch=newBatch;
        Marks=newmarks;
        Status=newstatus;
}

    function Studentdetails() public view returns(string,uint,uint,uint,string) {
         return(Name,RollNo,Batch,Marks,Status); 
}

}


